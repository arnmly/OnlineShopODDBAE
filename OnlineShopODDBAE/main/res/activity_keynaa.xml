<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B5AAA8"
    tools:context=".keyna">

    <ImageView
        android:id="@+id/back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/back" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="90dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="643dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="0dp"
        android:background="#000000"
        android:orientation="vertical"
        android:foregroundGravity="left"
        android:scrollbarAlwaysDrawVerticalTrack="true">

        <ImageView
            android:id="@+id/a"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@drawable/a" />
        <TextView
            android:id="@+id/aa"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="KEYNA SQUARE MAROON\nRp.55.000"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:textColor="#ffffff"/>

        <ImageView
            android:id="@+id/b"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@drawable/b" />

        <TextView
            android:id="@+id/bb"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="5dp"
            android:text="KEYNA SQUARE\nBEIGE\nRp.55.000"
            android:textAlignment="center"
            android:textColor="#ffffff" />

        <ImageView
            android:id="@+id/c"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@drawable/f" />

        <TextView
            android:id="@+id/cc"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="5dp"
            android:text="KEYNA SQUARE\nSILVER\nRp.60.000"
            android:textAlignment="center"
            android:textColor="#ffffff" />

        <ImageView
            android:id="@+id/g"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@drawable/g" />
        <TextView
            android:id="@+id/gg"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="KEYNA SQUARE\nTARO\nRp.60.000"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:textColor="#ffffff"/>

        <ImageView
            android:id="@+id/h"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@drawable/h" />
        <TextView
            android:id="@+id/hh"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="KEYNA SQUARE\nBLACK\nRp.60.000"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:textColor="#ffffff"/>

        <ImageView
            android:id="@+id/i"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@drawable/i" />
        <TextView
            android:id="@+id/ii"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="KEYNA SQUARE\nCREAM\nRp.60.000"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:textColor="#ffffff"/>

    </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/prof"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="27dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="25dp"
        android:background="@drawable/pro" />

</RelativeLayout>